<body ng-controller="follow" ng-cloak>
	<div class="homeTop">
		<div class="homeTopContainer">
			<div class="homeNavigation">
				<div class="container">
          <div class="homeTopbar">
            <div class="leftPanel">
              <ul>
                <li class="upperTab" style="border-bottom-color: #0084B4;    border-bottom: 5px solid #1b95e0;
    transition: all .15s ease-in-out; height:42px;">
    <ul style="padding: 0px;"><li><a href="/homepage" style="color: #66757f;font-size: 14px;">
                  <span class="Icon Icon--home Icon--large" style="float: left;margin: 11px 0 0 10px;width: 23px;height: 23px"></span></li><li>
                    <span class="text">Home</span></a>  </li></ul>
              </li>
               <li class="upperTab">
                <ul style="padding: 0px;"><li>
            <span class="Icon Icon--lightning Icon--large" style="float: left;margin: 11px 0 0 10px;width: 23px;height: 23px"></span></li><li>
            <span class="text">Moments</span></li></ul>
          </li>
              <li class="upperTab" >
                <ul style="padding: 0px;"><li>
                <span class="Icon Icon--notifications Icon--large" style="float: left;margin: 11px 0 0 10px;width: 23px;height: 23px"></span></li><li>
                <span class="text">Notifications</span></li></ul>

              
          </li>

          <li class="upperTab" >
            <ul style="padding: 0px;"><li>
                <span class="Icon Icon--dm Icon--large" style="float: left;margin: 11px 0 0 10px;width: 23px;height: 23px"></span></li><li>
                <span class="text">Messages</span>

              </li></ul>
          </li>

        </ul>
      </div>
      <div class="homeLogo" style="position:absolute;margin-left:70%;">
      <h1 class="Icon Icon--bird bird-topbar-etched" style="display: inline-block; width: 24px; height: 21px;color: #1da1f2;
    font-size: 21px;
    margin-top: 14px;
    margin-left: -24px;transition: all .25s ease-in-out;">
      <span class="visuallyhidden">Twitter</span>
      </h1>
      </div>
      <div class="rightPanel">
        <ul>
          <li>
            <a href="/search">
           <button type="submit" class="btn btn-primary btn-sm" style="margin-top: 7px; margin-right:7px;">Search</button> 
         </a>
          </li>
          <li>
            <a href="/logout">
             <button type="submit" class="btn btn-primary btn-sm" style="margin-top: 7px;">Log out</button> 
            </a>
          </li></ul>
        </div>
      </div>
      <div style="margin-top:50px;" layout="row" layout-wrap>
		<div flex="33" class="leftProfile">
          <div class="dashboardProfile">
           <div class="profileDisplayLeft">
            
               <img style="    background: #fff;
                                border: 5px solid #fff;
                                border-radius: 12px;
                                box-shadow: 0 1px 1px rgba(136,153,166,0.15);
                                position: relative; height:228px;width:228px;" src="../images/<%=username%>.jpeg" class="img-rounded">
                    <a href="/<%=username%>">
                    <h3 style="margin-left:13px;color:black;">{{userData.FIRST_NAME}} {{userData.LAST_NAME}}</h3></a>
                    
                    
                    <a href="/<%=username%>"><h4 style="padding-left:10px;margin-top:2px;">@<%=username%></h4></a> 
                    
                    <div class="profileStats">
                      <ul>
                    <li class="sizeFit">
        <a class="ProfileCardStats-statLink u-textUserColor u-linkClean u-block js-nav" title="3 Tweets" href="/homepage" data-element-term="tweet_stats">
          <span class="statLabel">Tweets</span>
          <span class="statValue">{{userData.TWEET_COUNT}}</span>
        </a>
      </li>
                      <li class="sizeFit">
          <a class="ProfileCardStats-statLink u-textUserColor u-linkClean u-block js-nav" title="135 Following" href="/follow" data-element-term="following_stats">
            <span class="statLabel">Following</span>
            <span class="statValue">{{userData.FOLLOWING_COUNT}}</span>
          </a>
        </li>
                      <li class="sizeFit">
          <a class="ProfileCardStats-statLink u-textUserColor u-linkClean u-block js-nav" title="36 Followers" href="/follow" data-element-term="follower_stats">
            <span class="statLabel">Followers</span>
            <span class="statValue">{{userData.FOLLOWER_COUNT}}</span>
          </a>
        </li>
                  </ul>
                    </div>
                    <a href="/editProfile">
                    <button type="submit" class="btn btn-primary btn-sm" style="margin-top: 7px; margin:13px; width:200px;" ng-click="editProfile();">
                      <span class="NewTweetButton-icon Icon Icon--editPencil Icon--medium"></span>
                      <span class="NewTweetButton-text" aria-hidden="true">Edit profile</span>
                    </button>
                  </a>


          </div> 
        </div>
        </div>        

         <md-divider></md-divider>
        <div flex="66" class="tabsdemoDynamicHeight">
        	<md-content>
    <md-tabs md-dynamic-height="" md-border-bottom="">
      <md-tab label="Following">
        <md-content>
            <md-list-item class="md-3-line md-long-text" ng-repeat="followingOne in following" style="margin:0px;" ng-show="isFollowingPresent">
             <img ng-src="../images/{{followingOne.TWITTER_HANDLE}}.jpeg" class="md-avatar" alt="">
                <div class="md-list-item-text">
                    <h3>{{followingOne.FIRST_NAME}} {{followingOne.LAST_NAME}}</h3> <h4><a href="/{{followingOne.TWITTER_HANDLE}}">@{{followingOne.TWITTER_HANDLE}}</a></h4>


                </div>
                <button type="submit" class="btn btn-primary btn-sm" style="margin-top:25px;" ng-click="follow();">
                     <ul style="padding:0px;"><li> <span class="NewTweetButton-icon Icon Icon--visibilityFollowing Icon--large"></span></li>
                      <li><span class="NewTweetButton-text" aria-hidden="true">Following</span></li></ul>
                    </button>
                <md-divider></md-divider>
            </md-list-item>
            <div ng-hide="isFollowingPresent">
            	<h3>No Followers</h3>
            </div>
        </md-content>
      </md-tab>
      <md-tab label="Followers">
        <md-content>
                  
            <md-list-item class="md-3-line md-long-text" ng-repeat="followerOne in follower" style="margin:0px;" ng-show="isFollowingPresent">
             <img ng-src="../images/{{followerOne.TWITTER_HANDLE}}.jpeg" class="md-avatar" alt="">
                <div class="md-list-item-text">
                    <h3>{{followerOne.FIRST_NAME}} {{followerOne.LAST_NAME}}</h3> <h4><a href="/{{followerOne.TWITTER_HANDLE}}">@{{followerOne.TWITTER_HANDLE}}</a></h4>


                </div>
                <button type="submit" class="btn btn-primary btn-sm" style="margin-top:25px;" ng-click="follow();" ng-show="{{followerOne.ALREADY_FOLLOWING}}==1">
                     <ul style="padding:0px;"><li> <span class="NewTweetButton-icon Icon Icon--visibilityFollowing Icon--large"></span></li>
                      <li><span class="NewTweetButton-text" aria-hidden="true">Following</span></li></ul>
                    </button>

                    <button type="submit" class="btn btn-primary btn-sm" style="margin-top:25px;" ng-click="follow();" ng-show="{{followerOne.ALREADY_FOLLOWING}}==0">
                     <ul style="padding:0px;"><li> <span class="NewTweetButton-icon Icon Icon--follow Icon--large"></span></li>
                      <li><span class="NewTweetButton-text" aria-hidden="true">Follow</span></li></ul>
                    </button>
                <md-divider></md-divider>
            </md-list-item>
            <div ng-hide="isFollowerPresent">
            	<h3>Not Following anyone</h3>
            </div>

        </md-content>
      </md-tab>
      
    </md-tabs>
  </md-content>

        </div>
